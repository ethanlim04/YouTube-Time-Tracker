{
  "manifest_version": 3,
  "name": "YouTube Time Tracker",
  "description": "YouTube watch time tracker",
  "version": "1.0.0",
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["storage", "tabs"],
  "host_permissions": ["https://www.youtube.com/*"]
}